{
	"name": "vsgencomments",
	"publisher": "VSCodeLamner",
	"repository": {"url": "https://github.com/kaminskg/VSCode-Generate-Comments"},
	"displayName": "Lamner Comment Generator",
	"description": "This is a Beta test of the \"Lamner Comment Generator\" extension.",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:java"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vsgencomments.insertComment",
				"title": "Generate Comment",
				"category": "Lamner"
			},
			{
				"command": "vsgencomments.install",
				"title": "Install Server",
				"category": "Lamner"
			},
			{
				"command": "vsgencomments.run",
				"title": "Run Server",
				"category": "Lamner"
			},
			{
				"command": "vsgencomments.shutdown",
				"title": "Shutdown Server",
				"category": "Lamner"
			}
		],
		"keybindings": [
			{
				"key": "ctrl+alt+;",
				"command": "vsgencomments.insertComment",
				"when": "textInputFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"node-fetch": "^2.6.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	}
}